{
  "name": "Kanmi Share",
  "description": "Share Links to a Kanmi API",
  "version": "0.0.2",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["contextMenus","storage","management"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon120.png"
  },
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Reload Configuration",
    "default_icon": "icon96.png"
  },
  "manifest_version": 2
}