{
  "name": "Kanmi Share",
  "description": "Share Links to Kanmi",
  "version": "0.0.1",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": ["contextMenus","storage","management"],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon120.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Reload Kanmi",
    "default_icon": "icon96.png"
  },
  "manifest_version": 2
}